## The function is currently defined as
function (x, y, t)
{
    outx <- 0
    outy <- 0
    n <- length(x) - 1
    for (i in 0:n) {
        outx <- outx + choose(n, i) * ((1 - t)^(n - i)) * t^i *
            x[i + 1]
        outy <- outy + choose(n, i) * ((1 - t)^(n - i)) * t^i *
            y[i + 1]
    }
    return(list(x = outx, y = outy))
  }
}
