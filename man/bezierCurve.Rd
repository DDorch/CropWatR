function (x, y, n = 10)
{
    outx <- NULL
    outy <- NULL
    i <- 1
    for (t in seq(0, 1, length.out = n)) {
        b <- bez(x, y, t)
        outx[i] <- b$x
        outy[i] <- b$y
        i <- i + 1
    }
    return(list(x = outx, y = outy))
  }
}
